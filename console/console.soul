// Console module for Soul - provides console output functionality
// Similar to JavaScript's console object

// Export Console object directly
Console = {
    "log": soul(message) {
        println(message)
    },
    
    "println": soul(message) {
        println(message)
    },
    
    "info": soul(message) {
        println("ℹ️  " + message)
    },
    
    "warn": soul(message) {
        println("⚠️  " + message)
    },
    
    "error": soul(message) {
        println("❌ " + message)
    },
    
    "debug": soul(message) {
        println("🐛 " + message)
    }
}