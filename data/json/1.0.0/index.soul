// Built-in Json library wrapping json module
static sanctuary Json {

    soul parse(jsonString) {
        return JSON.decode(jsonString)
    }

    soul stringify(object) {
        return JSON.encode(object)
    }

    soul decode(jsonString) {
        return JSON.decode(jsonString)
    }

    soul encode(object) {
        return JSON.encode(object)
    }
}
